{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/lib/clubradar/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/lib/clubradar/wsgi.py"
    }
  ]
}
