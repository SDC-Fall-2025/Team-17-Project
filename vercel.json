{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/pkg/clubradar/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "cwd": "api"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/pkg/clubradar/wsgi.py"
    }
  ]
}
